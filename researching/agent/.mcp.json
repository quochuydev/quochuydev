{
  "mcpServers": {
    "x-agent": {
      "command": "docker",
      "args": [
        "run",
        "-i",
        "--rm",
        "--network",
        "host",
        "-e",
        "NEO4J_URI=bolt://localhost:7687",
        "-e",
        "NEO4J_USER=neo4j",
        "-e",
        "NEO4J_PASSWORD=password",
        "-e",
        "OPENAI_API_KEY=${OPENAI_API_KEY}",
        "quochuydev/core-x-agent:0.0.1"
      ]
    }
  }
}
